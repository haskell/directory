{
    "problemMatcher": [
        {
            "owner": "directory",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+)[^:]*: (\\w+): (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
